{
  "NatsConfiguration": {
    "Url": "nats://nats:4222",
    "PullSubject": "gateway.config.pull",
    "MessageSubjectTemplate": "messages.gateway.{0}",
    "MaxRetryAttempts": 7,
    "RetryBackoffSeconds": 2,
    "MaxReconnect": -1,
    "ReconnectWait": 30000,
    "AllowReconnect": true,
    "MaxReconnectAttempts": 100,
    "ReconnectIntervalMs": 30000,
    "MaxInitialRetries": 20,
    "InitialRetryIntervalMs": 30000
  },
  "GatewayAppSettings": {
    "ConfigFilePathBase": "config_gateway",
    "ConfigFilePathExt": "json"
  }
}